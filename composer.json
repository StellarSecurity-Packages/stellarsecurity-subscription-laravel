{
    "name": "stellarsecurity/subscription-laravel",
    "description": "Stellar Security Subscription client for PHP/Laravel. Simple wrapper around the Stellar Subscription API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stellar Security",
            "homepage": "https://stellarsecurity.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "StellarSecurity\\\\SubscriptionLaravel\\\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "StellarSecurity\\\\SubscriptionLaravel\\\\Providers\\\\SubscriptionServiceProvider"
            ],
            "aliases": {
                "StellarSubscription": "StellarSecurity\\\\SubscriptionLaravel\\\\Facades\\\\Subscription"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}